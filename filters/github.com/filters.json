[
  {
    "page": "/:owner/:repo/issues/:id",
    "filter": {
      "field": "url",
      "op": "contains",
      "value": "/:owner/:repo/issues/:id#issue"
    }
  },
  {
    "page": "/:owner/:repo/pulls",
    "filter": {
      "field": "pathname",
      "op": "startsWith",
      "value": "/:owner/:repo/pull/"
    }
  },
  {
    "page": "/:owner/:repo/releases",
    "filter": {
      "field": "pathname",
      "op": "startsWith",
      "value": "/:owner/:repo/releases/tag/"
    }
  }
]
